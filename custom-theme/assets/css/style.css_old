/*--- Custom CSS ---*/

/* --header */
header {border-bottom: 4px solid #e2b30d;top: -10%;background: #000; display: block; width: 100%;background: -webkit-gradient(linear, left top, left bottom, color-stop(0, #000),to(#242424));background: linear-gradient(to bottom, #000 0,#242424 100%);filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#000', endColorstr='#242424',GradientType=0 );top: 0;z-index: 99;left: 0;right: 0;-webkit-transition: 500ms all;transition: 500ms all;}
header:after,header:before{content: "";display: table;width: 100%;}

.navMain nav ul li {display: inline-block;line-height: 30px;position: relative;-webkit-transition: 300ms all;transition: 300ms all;}
.logoMain {float: left;padding-top: 26px;}
.navMain {float: right;}
.navMain nav ul li a {color: #fff;text-transform: uppercase;position: relative;font-weight: 600;font-size: 18px;z-index: 9;display: block; padding: 26px 43px 26px 60px;}
.navMain nav ul li a:before {content: "";height: 13px;width: 5px;background: #e2b30d;position: absolute;top: 50%;left: 42px;-webkit-transform: skew(-31deg);transform: skew(-31deg);margin-top: -7px;}
.navMain nav ul li:before, .navMain nav ul li:last-child:after {content: "";background: #383838;width: 1px;position: absolute;top: 0;-webkit-transform: skew(-31deg);transform: skew(-31deg);height:100%;}
.navMain nav ul li:last-child:after {right: 0;}
.navMain nav ul li:before{left: 0;}
.navMain nav ul li:after{height: 0}
.navMain nav ul li a:hover:after {background: #e2b30d;content: "";height: 100%;width: 102%;left: 0;position: absolute;top: 0;-webkit-transform: skew(-31deg);transform: skew(-31deg);right: 0;z-index: -1;    }
.navMain nav ul li a:hover{color: #000;}
.navMain nav ul li a:hover:before{background: #000;}
.parallax-mirror{background-size: cover !important;}
/* --mypro */
.startPro:before, .startProBottom:before{position: absolute;height: 100%;width: 100%;background: rgba(0, 0, 0, 0.7);content: "";top: 0;}
.startProBottom.parallax-window {min-height: 762px;background: transparent;position: relative;}
